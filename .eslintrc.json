{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["warn", { "varsIgnorePattern": "^_", "args": "none", "ignoreRestSiblings": true }], 
    "@typescript-eslint/no-empty-interface": ["warn", { "varsIgnorePattern": "^_", "args": "none", "ignoreRestSiblings": true }]
    
  }
}